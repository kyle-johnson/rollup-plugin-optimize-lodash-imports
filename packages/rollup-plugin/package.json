{
  "name": "@optimize-lodash/rollup-plugin",
  "version": "5.1.0",
  "description": "Rewrite lodash imports with Rollup for improved tree-shaking.",
  "keywords": [
    "lodash",
    "rollup",
    "rollup-plugin",
    "vite-plugin",
    "rolldown",
    "optimize",
    "minify"
  ],
  "homepage": "https://github.com/kyle-johnson/rollup-plugin-optimize-lodash-imports/tree/main/packages/rollup-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyle-johnson/rollup-plugin-optimize-lodash-imports.git"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Kyle Johnson",
  "license": "MIT",
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "test:ci": "jest --coverage --ci",
    "test:rolldown": "tsx tests/rolldown/rolldown.mts",
    "build": "rm -rf dist && tsc -p tsconfig.dist.json && gen-esm-wrapper ./dist ./dist/index.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "depcheck": "depcheck"
  },
  "peerDependencies": {
    "rollup": ">= 4.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@tsconfig/node18": "catalog:",
    "@types/estree": "1.0.1",
    "@types/jest": "catalog:",
    "@types/lodash": "catalog:",
    "@types/node": "18.19.74",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "depcheck": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "55.0.0",
    "gen-esm-wrapper": "1.1.3",
    "jest": "catalog:",
    "lodash": "catalog:",
    "lodash.kebabcase": "4.1.1",
    "prettier": "catalog:",
    "rolldown": "1.0.0-beta.51",
    "@rolldown/binding-wasm32-wasi": "1.0.0-beta.51",
    "rollup": "4.52.5",
    "ts-jest": "catalog:",
    "typescript": "catalog:",
    "tsx": "catalog:"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@optimize-lodash/transform": "workspace:3.0.6"
  }
}
