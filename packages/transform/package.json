    "@types/node": "12.20.55",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "acorn": "8.14.0",
    "depcheck": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-jest": "catalog:",
    "eslint-plugin-unicorn": "44.0.2",
    "jest": "catalog:",
    "prettier": "catalog:",
    "ts-jest": "catalog:",
    "typescript": "catalog:",
    "tsx": "catalog:"
  },
  "dependencies": {
    "estree-walker": "^2.0.2",
    "magic-string": "~0.30.11"
  }
